<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data  noupdate="1">
        <record id="ir_cron_accounting_invoice_cancel" model="ir.cron">
        <field name="name">Accounting Invoice Cron Cancel</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">24</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(hours=24)).strftime('%Y-%m-%d 00:00:00')" />
        <field name="interval_type">hours</field>
        <field eval="False" name="doall"/>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="'account.invoice'" name="model" />
        <field eval="'invoice_autocancel'" name="function" />
        <field eval="'(None,)'" name="args" />
        <field name="priority">1000</field>
</record>
    </data>
</odoo>